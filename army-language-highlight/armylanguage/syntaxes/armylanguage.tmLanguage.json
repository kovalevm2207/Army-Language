{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ArmyLanguage",
	"patterns": [
		{ "include": "#brackets" },
		{ "include": "#parentheses" },
		{ "include": "#comments" },
		{ "include": "#keywords" },
		{ "include": "#function-declarations" },
		{ "include": "#builtin-functions" },
		{ "include": "#main-function" },
		{ "include": "#user-functions" },
		{ "include": "#loops" },
		{ "include": "#conditionals" },
		{ "include": "#constant-declarations" },
		{ "include": "#variables" },
		{ "include": "#numbers" },
		{ "include": "#operators" },
		{ "include": "#separators" },
		{ "include": "#returns" }
	],
	"repository": {
		"comments": {
			"patterns": [{
				"name": "comment.line.armylanguage",
				"begin": "\\$\\$",
				"end": "\n"
			}]
		},
		"parentheses": {
			"patterns": [{
				"name": "punctuation.parenthesis.begin.armylanguage",
				"match": "Быстро_на"
			}, {
				"name": "punctuation.parenthesis.end.armylanguage",
				"match": "Кому_сказал_на"
			}]
		},
		"brackets": {
			"patterns": [{
				"name": "meta.block.armylanguage",
				"begin": "Только_попробуйте_сделать_по_другому",
				"end": "Ноги_руки_оторву",
				"patterns": [
					{ "include": "$self" }
				]
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.armylanguage",
				"match": "(Становись\\!|Равняйсь\\!|Смирно\\!|Равнение_на_середину\\!|Вольно\\!|Разойдись\\!)"
			}]
		},
		"function-declarations": {
			"patterns": [{
				"name": "entity.name.function.declaration.armylanguage",
				"match": "\\b(Слушай_боевую_задачу\\!)\\b"
			}]
		},
		"builtin-functions": {
			"patterns": [{
				"name": "support.function.builtin.armylanguage",
				"match": "\\b(Громко_и_четко_назови|Разведать_обстановку)\\b"
			}]
		},
		"main-function": {
			"patterns": [{
				"name": "entity.name.function.main.armylanguage",
				"match": "\\b(ВыполнитьПриказГенерала)\\b"
			}]
		},
		"user-functions": {
			"patterns": [{
				"name": "entity.name.function.user.armylanguage",
				"match": "\\b(МозолитьГлаза|ВыполнитьПриказПолковника|ВыполнитьПриказПодполковника|ВыполнитьПриказМайора|ВыполнитьПриказКапитана|ВыполнитьПриказСтаршегоЛейтенента|ВыполнитьПриказЛейтенента|ВыполнитьПриказМладшегоЛейтенанта|ВыполнитьПриказСтаршегоПрапорщика|ВыполнитьПриказПрапорщика|ВыполнитьПриказСтаршины|ВыполнитьПриказМладшегоПрапорщика|СтоятьНаСмерть|БуксоватьЕбалом|СтоятьКакБыкНассал|НайтиПричину|НайтиШпиона|ОтразитьАтаку|ЗанятьВысоту|ОбеспечитьПрикрытие|ПровестиРазведку|УничтожитьЦель|ОрганизоватьОборону|ПодготовитьЗасаду|ЭвакуироватьРаненых|ЗахватитьПленных|СкрытьСледы|ПодорватьОбъект|ПередатьШифровку|ЗакрепитьПозиции|ОтправитьДозор|СвязатьсяСЦентром|ЗаблокироватьПодходы|СоздатьПомехи|ОпределитьКоординаты|ПодавитьОгонь|ВзорватьМост|ЗаминироватьУчасток|ВзятьЯзыка|СымитироватьАтаку|СменитьПозицию|УстановитьНаблюдение|СфальсифицироватьДокументы|ОбезвредитьМину|НавестиАртиллерию|ЗапуститьДрон|РасширитьПлацдарм|ЗахватитьДокументы|СоздатьДиверсию|ОбнаружитьЗасаду|ТихоСпиздитьИУйти|НарватьсяНаПатруль|ПроебатьАвтомат|НайтиПриключенийНаЗадницу|ВозомнитьСебяУмным|ЗанятьПлацдарм|ПодсчитатьПотери|ОценитьСитуацию|МобилизоватьРезервы|КоординироватьУдар|МаскироватьТехнику|ПроникнутьВШтаб|ВывестиИзСтроя|СобратьДанные|ДоложитьПоЦепочке|ЗапуститьРакету|РасшифроватьПерехват|РассчитатьТраекторию|ПодтвердитьПароль|ОткрытьШквальныйОгонь|ЗаложитьВзрывчатку|ПровестиКонтрразведку|ЗахватитьПередачу|НейтрализоватьСнайпера|ПрорватьОборону|ОкружитьПротивника|ОтбитьАтаку|ЗанятьКлючевуюТочку|ВестиНаблюдение|ЗасечьПередвижения|СобратьРазведданные|КонтролироватьУчасток|ОбнаружитьТехнику|ДоставитьБоеприпасы|ВосстановитьСвязь|ЭвакуироватьТехнику|СоздатьЗапаснойПункт|КоординироватьПодкрепление|ПроникнутьВТылы|ДезинформироватьПротивника|ЗахватитьШифровальщика|СоздатьЛожнуюЦель|ВывестиИзСтрояСистему|ПротестироватьРекурсию|ПоказатьВложенность|Демо_ПриоритетОператоров|Демо_ВложенныеУсловия|Демо_ЦепочкаВызовов|Атаковать|Защищаться|Ждать|Двигаться|Стрелять|Скрыться|Преследовать|Отступать|Анализировать|Реагировать|ЗаложитьМины|СнятьЧасовых|ПерехватитьСообщение|СфабриковатьДоказательства|ИнфильтроватьсяВРяды|СорватьПереговоры|ЗаблокироватьДороги|СоздатьУчебнуюТревогу|ПровестиУчения|ОтчитатьсяОРезультатах|ЗапроситьПоддержку|СкорректироватьОгонь|ОбойтиСФланга|УстроитьЗавалы|ПоджечьСклады|ОтравитьВодоем|ВыкрастьЧертежи|ВнедритьАгента|СкомпрометироватьКомандира|УстроитьБунт|СымитироватьПоломку|ПодкупитьЧасового|УстроитьДиверсиюНаЖД|ПеререзатьЛинииСвязи|УничтожитьСклады|ЗахватитьРадиостанцию|ВзятьВКольцо|ВыставитьПосты|ПроверитьДокументы|ОбыскатьМестность|ЗадержатьПодозреваемого|ДопроситьПленного|СвернутьЛагерь|РазвернутьШтаб|НавестиПорядок|ВыдатьПатроны|ПровестиСмотр|ОтправитьДонесение|ПринятьШифровку|СменитьКодовоеСлово|ЗапуститьДымовуюЗавесу|ПоставитьМинометы|ЗанятьОкопы|ВырытьУкрытия|УстановитьКолючку|НатянутьМаскировку|ПодвезтиПродовольствие)\\b"
			}]
		},
		"loops": {
			"patterns": [{
				"name": "keyword.control.loop.armylanguage",
				"match": "\\b(Пидорасишь)\\b"
			}]
		},
		"conditionals": {
			"patterns": [{
				"name": "keyword.control.conditional.armylanguage",
				"match": "\\b(Не_дай_бог_блять)\\b"
			}]
		},
		"constant-declarations": {
			"patterns": [{
				"name": "variable.other.constant.armylanguage",
				"match": "\\b(С_этого_момента)\\b"
			}]
		},
		"variables": {
			"patterns": [{
				"name": "variable.other.armylanguage",
				"match": "\\b(Солдат|ДлинаШеренги|ДлинаКолонны|Широта|Долгота|ПозицияВрага|СигналТревоги|Рота|Взвод|Отделение|Полк|Батальон|Дивизия|Армия|Отряд|Высота|Азимут|Дистанция|Сектор|Рубка|Часовой|Дозорный|Снайпер|Сапер|Связист|Разведчик|Пулеметчик|Гранатометчик|Медик|Повар|Боезапас|Патроны|Гранаты|Мины|Взрывчатка|Продовольствие|Вода|Топливо|Бинокль|Рация|Карта|Компас|Часы|Фонарь|Нож|Лопатка|Плащпалатка|Противогаз|Аптечка|Жгут|КоординатаX|КоординатаY|Скорость|Время|Температура|Видимость|Влажность|Давление|Ветер|Осадки|Укрепление|Окоп|Блиндаж|Дзот|Каземат|Траншея|ХодСообщения|ПостНаблюдения|КПП|Караул|Пароль|Отзыв|КодовоеСлово|Шифр|Ключ|Шифроблокнот|Радиочастота|Позывной|Пеленг|Засечка|Урон|Потери|Раненые|Убитые|Пленные|Трофеи|Захваченное|Уничтоженное|Поврежденное|Списанное|Мораль|Дисциплина|Усталость|Голод|Жажда|Обморожение|Травма|Болезнь|Инфекция|Карантин|Зарплата|Премия|Надбавка|Штраф|Взыскание|Поощрение|Награда|Орден|Медаль|Значок|Срок|Очередь|Очередность|Приоритет|ОчередьНаОтпуск|ОчередьНаПовышение|Стаж|Звание|Должность|Специальность|Очко|Шухер|Дневальный|Дежурный|Комбат|Гражданские|Ракеты|Магазины|Рана|Маршбросок)\\b"
			}]
		},
		"numbers": {
			"patterns": [{
				"name": "constant.numeric.armylanguage",
				"match": "\\b\\d+\\b"
			}]
		},
		"operators": {
			"patterns": [{
				"name": "keyword.operator.armylanguage",
				"match": "\\b(Есть_ничто_иное_как|Неотъемлемая_часть|Руки_убрал|Больше_нарядов_вне_очереди|Только_не_включай|Укради_роди_найди|Меньше_можно_больше_нельзя|Перекрестие|Себе_в_минус|Не_звезди|Отбива|Вражеская_разработка)\\b"
			}]
		},
		"separators": {
			"patterns": [{
				"name": "punctuation.terminator.armylanguage",
				"match": "(\\bЧтобы\\b|\\bПриступаем_к_операции\\b|Уяснил_солдат\\!\\?)"
			}]
		},
		"returns": {
			"patterns": [{
				"name": "keyword.control.return.armylanguage",
				"match": "\\b(Только_попробуйте_съебаться_с)\\b"
			}]
		}
	},
	"scopeName": "source.armylanguage"
}
